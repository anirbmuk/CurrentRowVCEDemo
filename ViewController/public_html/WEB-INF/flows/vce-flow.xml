<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="vce-flow">
    <default-activity>VCEDemoView</default-activity>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>backingBean</managed-bean-name>
      <managed-bean-class>blog.anirbanm.vcerow.view.bean.BackingManager</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="VCEDemoView">
      <page>/WEB-INF/fragments/VCEDemoView.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
